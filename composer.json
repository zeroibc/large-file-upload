{
    "name": "zeroibc/large-file-upload",
    "description": "An extension of laravel-admin for uploading large files to qiniu",
    "type": "library",
    "keywords": ["laravel-admin", "extension"],
    "homepage": "https://github.com/zeroibc/large-file-upload",
    "license": "MIT",
    "authors": [
        {
            "name": "zeroibc",
            "email": "zeroibc@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Zero\\LargeFileUpload\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zero\\LargeFileUpload\\LargeFileUploadServiceProvider"
            ]

        }
    }
}
